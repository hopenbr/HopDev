                            <Control FieldName="HIC.Team.Build.Type2" Type="FieldControl" Label="Team Build Type:" LabelPosition="Left" ReadOnly="True" />
                          </Tab>
                          <Tab Label="BPI3">
                            <Control FieldName="HIC.Team.Build.Project3" Type="FieldControl" Label="Team Project:" LabelPosition="Left" ReadOnly="True" />
                            <Control FieldName="HIC.Team.Build.Type3" Type="FieldControl" Label="Team Build Type:" LabelPosition="Left" ReadOnly="True" />
                          </Tab>
                          <Tab Label="BPI4">
                            <Control FieldName="HIC.Team.Build.Project4" Type="FieldControl" Label="Team Project:" LabelPosition="Left" ReadOnly="True" />
                            <Control FieldName="HIC.Team.Build.Type4" Type="FieldControl" Label="Team Build Type:" LabelPosition="Left" ReadOnly="True" />
                          </Tab>
                          <Tab Label="BPI5">
                            <Control FieldName="HIC.Team.Build.Project5" Type="FieldControl" Label="Team Project:" LabelPosition="Left" ReadOnly="True" />
                            <Control FieldName="HIC.Team.Build.Type5" Type="FieldControl" Label="Team Build Type:" LabelPosition="Left" ReadOnly="True" />
                          </Tab>
                          <Tab Label="BPI6">
                            <Control FieldName="HIC.Team.Build.Project6" Type="FieldControl" Label="Team Project:" LabelPosition="Left" ReadOnly="True" />
                            <Control FieldName="HIC.Team.Build.Type6" Type="FieldControl" Label="Team Build Type:" LabelPosition="Left" ReadOnly="True" />
                          </Tab>
                          <Tab Label="BPI7">
                            <Control FieldName="HIC.Team.Build.Project7" Type="FieldControl" Label="Team Project:" LabelPosition="Left" ReadOnly="True" />
                            <Control FieldName="HIC.Team.Build.Type7" Type="FieldControl" Label="Team Build Type:" LabelPosition="Left" ReadOnly="True" />
                          </Tab>
                          <Tab Label="BPI8">
                            <Control FieldName="HIC.Team.Build.Project8" Type="FieldControl" Label="Team Project:" LabelPosition="Left" ReadOnly="True" />
