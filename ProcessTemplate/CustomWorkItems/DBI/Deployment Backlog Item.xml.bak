                <Control FieldName="HIC.Deploy.Date" Type="DateTimeControl" Label="Deployment Date:" LabelPosition="Left" Format="Custom" CustomFormat="MM/dd/yyyy" />
                <Control FieldName="HIC.Deployment.Time" Type="FieldControl" Label="Deployment Time:" LabelPosition="Left" />
