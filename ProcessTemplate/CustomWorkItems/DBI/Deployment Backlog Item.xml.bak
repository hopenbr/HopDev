                        <Control FieldName="HIC.Build.Package3" Type="FieldControl" Label="3: " LabelPosition="Left" />
                        <Control FieldName="HIC.Build.Package4" Type="FieldControl" Label="4: " LabelPosition="Left" />
                        <Control FieldName="HIC.Build.Package5" Type="FieldControl" Label="5:" LabelPosition="Left" />
                        <Control FieldName="HIC.Build.Package6" Type="FieldControl" Label="6: " LabelPosition="Left" />
                        <Control FieldName="HIC.Build.Package7" Type="FieldControl" Label="7: " LabelPosition="Left" />
                        <Control FieldName="HIC.Build.Package8" Type="FieldControl" Label="8: " LabelPosition="Left" />
                      </Column>
                    </Group>
                  </Column>
                  <Column PercentWidth="50">
                    <Group Label="Build Package Info">
                      <Column PercentWidth="100">
                        <TabGroup>
                          <Tab Label="BPI1">
