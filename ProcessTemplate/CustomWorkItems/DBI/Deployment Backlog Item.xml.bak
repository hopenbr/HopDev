    <WORKFLOW>
      <STATES>
        <STATE value="Not Done" />
        <STATE value="Deferred" />
        <STATE value="Deleted" />
        <STATE value="Rejected" />
        <STATE value="Approved for Deployment" />
        <STATE value="Approved by IRB" />
        <STATE value="Ready for IRB (Production Only)" />
        <STATE value="Rolled Back" />
        <STATE value="Deployed" />
      </STATES>
      <TRANSITIONS>
        <TRANSITION from="" to="Not Done">
          <REASONS>
            <DEFAULTREASON value="New" />
          </REASONS>
          <FIELDS>
            <FIELD refname="Conchango.VSTS.Scrum.Audit">
              <DEFAULT from="value" value="0" />
            </FIELD>
          </FIELDS>
        </TRANSITION>
        <TRANSITION from="Deferred" to="Deleted">
          <REASONS>
            <DEFAULTREASON value="Item has been marked for deletion" />
          </REASONS>
          <FIELDS>
            <FIELD refname="Conchango.VSTS.Scrum.Audit">
              <DEFAULT from="value" value="0" />
            </FIELD>
          </FIELDS>
        </TRANSITION>
        <TRANSITION from="Deferred" to="Not Done">
          <REASONS>
            <DEFAULTREASON value="Item has been marked for restoration" />
          </REASONS>
          <FIELDS>
            <FIELD refname="Conchango.VSTS.Scrum.Audit">
              <DEFAULT from="value" value="0" />
            </FIELD>
          </FIELDS>
        </TRANSITION>
        <TRANSITION from="Not Done" to="Deferred">
          <REASONS>
            <DEFAULTREASON value="Descoped from deployments" />
          </REASONS>
          <FIELDS>
            <FIELD refname="Conchango.VSTS.Scrum.Audit">
              <DEFAULT from="value" value="0" />
            </FIELD>
          </FIELDS>
        </TRANSITION>
        <TRANSITION from="Not Done" to="Approved for Deployment">
          <REASONS>
            <DEFAULTREASON value="The package was approved by the team led for deployment" />
          </REASONS>
          <FIELDS>
            <FIELD refname="Conchango.VSTS.Scrum.Audit">
              <DEFAULT from="value" value="0" />
            </FIELD>
          </FIELDS>
        </TRANSITION>
        <TRANSITION from="Approved for Deployment" to="Deferred">
          <REASONS>
            <DEFAULTREASON value="Descoped from deployment backlog" />
          </REASONS>
          <FIELDS>
            <FIELD refname="Conchango.VSTS.Scrum.Audit">
              <DEFAULT from="value" value="0" />
            </FIELD>
          </FIELDS>
        </TRANSITION>
        <TRANSITION from="Deleted" to="Not Done">
          <REASONS>
            <DEFAULTREASON value="Undo/Undelete" />
          </REASONS>
          <FIELDS>
            <FIELD refname="Conchango.VSTS.Scrum.Audit">
              <DEFAULT from="value" value="0" />
            </FIELD>
          </FIELDS>
        </TRANSITION>
        <TRANSITION from="Ready for IRB (Production Only)" to="Approved by IRB">
          <REASONS>
            <DEFAULTREASON value="The package was approved by the Internal Review Board for deployment tp Production" />
          </REASONS>
          <FIELDS>
            <FIELD refname="Conchango.VSTS.Scrum.Audit">
              <DEFAULT from="value" value="0" />
            </FIELD>
          </FIELDS>
        </TRANSITION>
        <TRANSITION from="Approved for Deployment" to="Ready for IRB (Production Only)" for="[global]\Team Foundation Administrators">
          <REASONS>
            <DEFAULTREASON value="Build package was sent to IRB for Approval" />
          </REASONS>
          <FIELDS>
            <FIELD refname="Conchango.VSTS.Scrum.Audit">
              <DEFAULT from="value" value="0" />
            </FIELD>
          </FIELDS>
        </TRANSITION>
        <TRANSITION from="Not Done" to="Rejected">
          <REASONS>
            <DEFAULTREASON value="The package was rejected by the team led for deployment" />
          </REASONS>
          <FIELDS>
            <FIELD refname="Conchango.VSTS.Scrum.Audit">
              <DEFAULT from="value" value="0" />
            </FIELD>
          </FIELDS>
        </TRANSITION>
        <TRANSITION from="Rejected" to="Not Done">
          <REASONS>
            <DEFAULTREASON value="Rejected DBI was brought back to backlog for deployment" />
          </REASONS>
          <FIELDS>
            <FIELD refname="Conchango.VSTS.Scrum.Audit">
              <DEFAULT from="value" value="0" />
            </FIELD>
          </FIELDS>
        </TRANSITION>
        <TRANSITION from="Rolled Back" to="Ready for IRB (Production Only)">
          <REASONS>
            <DEFAULTREASON value="Rolled back build is ready to be moved back to PRD" />
          </REASONS>
          <FIELDS>
            <FIELD refname="Conchango.VSTS.Scrum.Audit">
              <DEFAULT from="value" value="0" />
            </FIELD>
          </FIELDS>
        </TRANSITION>
        <TRANSITION from="Ready for IRB (Production Only)" to="Rejected">
          <REASONS>
            <DEFAULTREASON value="The package was rejected by the Internal Review Board" />
          </REASONS>
          <FIELDS>
            <FIELD refname="Conchango.VSTS.Scrum.Audit">
              <DEFAULT from="value" value="0" />
            </FIELD>
          </FIELDS>
        </TRANSITION>
        <TRANSITION from="Approved for Deployment" to="Rejected">
          <REASONS>
            <DEFAULTREASON value="Deployment Rejected" />
          </REASONS>
          <FIELDS>
            <FIELD refname="Conchango.VSTS.Scrum.Audit">
              <DEFAULT from="value" value="0" />
            </FIELD>
          </FIELDS>
        </TRANSITION>
        <TRANSITION from="Approved for Deployment" to="Deployed">
          <REASONS>
            <DEFAULTREASON value="The Build package was deployed to the Target Environment" />
          </REASONS>
          <FIELDS>
            <FIELD refname="Conchango.VSTS.Scrum.Audit">
              <DEFAULT from="value" value="0" />
            </FIELD>
          </FIELDS>
        </TRANSITION>
        <TRANSITION from="Approved by IRB" to="Deployed">
          <REASONS>
            <DEFAULTREASON value="The Build package was deployed from the Team Project's TFVC Release area to Production" />
          </REASONS>
          <FIELDS>
            <FIELD refname="Conchango.VSTS.Scrum.Audit">
              <DEFAULT from="value" value="0" />
            </FIELD>
          </FIELDS>
        </TRANSITION>
        <TRANSITION from="Approved by IRB" to="Deferred">
          <REASONS>
            <DEFAULTREASON value="The production deployment was deferred" />
          </REASONS>
          <FIELDS>
            <FIELD refname="Conchango.VSTS.Scrum.Audit">
              <DEFAULT from="value" value="0" />
            </FIELD>
          </FIELDS>
        </TRANSITION>
        <TRANSITION from="Deployed" to="Rolled Back">
          <REASONS>
            <DEFAULTREASON value="The Production build was Rolled back to a previous build" />
          </REASONS>
          <FIELDS>
            <FIELD refname="Conchango.VSTS.Scrum.Audit">
              <DEFAULT from="value" value="0" />
            </FIELD>
          </FIELDS>
        </TRANSITION>
        <TRANSITION from="Rolled Back" to="Not Done">
          <REASONS>
            <DEFAULTREASON value="Rolled back build is ready to be deployed again" />
          </REASONS>
          <FIELDS>
            <FIELD refname="Conchango.VSTS.Scrum.Audit">
              <DEFAULT from="value" value="0" />
            </FIELD>
          </FIELDS>
        </TRANSITION>
        <TRANSITION from="Rolled Back" to="Deleted">
          <REASONS>
            <DEFAULTREASON value="Rolled back build is no longer needed" />
          </REASONS>
          <FIELDS>
            <FIELD refname="Conchango.VSTS.Scrum.Audit">
              <DEFAULT from="value" value="0" />
            </FIELD>
          </FIELDS>
        </TRANSITION>
        <TRANSITION from="Approved by IRB" to="Rejected">
          <REASONS>
            <DEFAULTREASON value="Re-approval needed" />
          </REASONS>
          <FIELDS>
            <FIELD refname="Conchango.VSTS.Scrum.Audit">
              <DEFAULT from="value" value="0" />
            </FIELD>
          </FIELDS>
        </TRANSITION>
      </TRANSITIONS>
    </WORKFLOW>
    <FORM>
      <Layout>
        <Group>
