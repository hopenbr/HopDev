                  <Column PercentWidth="50">
                    <Group Label="Build Packages">
                      <Column PercentWidth="100">
                        <Control FieldName="HIC.Build.Number" Type="FieldControl" Label="Build/Package Number:" LabelPosition="Left" Padding="(0,0,0,25)" />
