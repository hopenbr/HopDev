                      </Column>
                    </Group>
                  </Column>
                </Group>
                <Group Label="Dependency Infomation">
                  <Column PercentWidth="50">
                    <Control FieldName="HIC.Dependency" Type="FieldControl" Label="Deployment dependencies:" LabelPosition="Left" Dock="Top" />
                  </Column>
                  <Column PercentWidth="50">
                    <Control FieldName="HIC.Dependency.ProjectTeam" Type="FieldControl" Label="Project you are dependent on:" LabelPosition="Left" />
                  </Column>
                </Group>
                <Control FieldName="System.Description" Type="HtmlFieldControl" Label="Deployment and Dependencies Description:" LabelPosition="Top" Dock="Fill" />
              </Tab>
              <Tab Label="Build/Package">
