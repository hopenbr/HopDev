      <Target>
        <stateMoniker Name="/Deployment Backlog Item/Deferred" />
      </Target>
      <Reasons>
        <reason Name="Descoped from deployment backlog" />
      </Reasons>
      <fields>
        <transitionFieldConstraint ruleTag="* ">
          <fieldMoniker Name="/Deployment Backlog Item/Audit" />
        </transitionFieldConstraint>
      </fields>
    </transition>
