    <transition Id="1b189598-2f45-4e8d-b1be-7a4a662f90ed" Name="Rolled Back~Ready for IRB (Production Only)">
      <Target>
        <stateMoniker Name="/Deployment Backlog Item/Ready for IRB (Production Only)" />
