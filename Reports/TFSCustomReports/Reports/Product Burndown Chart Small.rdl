      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>---------------------------------------------------------- Test data
--DECLARE @vProjectName AS varchar(20)
--SET @vProjectName= '/Mark012';
----------------------------------------------------------End of test data

DECLARE @work AS varchar(1000)
SET @work='';

-- ----------------------------------------------------------
-- Select Product Backlog history where the WorkRemaining is zero
-- ----------------------------------------------------------
WITH CTE1 AS
(
SELECT     YEAR(h.Date)*10000+MONTH(h.Date)*100+DAY(h.Date)  AS  StartDate,
                 w.System_Id AS SystemId
FROM           [Work Item History] AS h
INNER JOIN [Work Item] AS w ON h.[Work Item] = w.__ID
INNER JOIN [Team Project] AS t ON h.[Team Project] = t.__ID
WHERE    w.System_WorkItemType = 'Product Backlog Item' AND '/'+t.[Team Project]+'/Portal Reports'  = @vProjectName
AND         w.System_State != 'Deleted'
AND         h.Conchango_VSTS_Scrum_WorkRemaining = 0
),

-- ----------------------------------------------------------
-- Where there are duplicates, choose the earliest
-- i.e. get the first date when each Product Backlog was seen to be completed
-- ----------------------------------------------------------
CTE2 AS
(
SELECT Min(StartDate) AS StartDate, SystemId FROM CTE1 GROUP BY SystemId
),

-- ----------------------------------------------------------
-- Meanwhile, get the Sprints...
-- ----------------------------------------------------------
CTE3 AS
(
SELECT     YEAR(c.Conchango_VSTS_Scrum_SprintStart)*10000+
                 MONTH(c.Conchango_VSTS_Scrum_SprintStart)*100+
                 DAY(c.Conchango_VSTS_Scrum_SprintStart) As SprintStart,
                YEAR(c.Conchango_VSTS_Scrum_SprintEnd)*10000+
                 MONTH(c.Conchango_VSTS_Scrum_SprintEnd)*100+
                 DAY(c.Conchango_VSTS_Scrum_SprintEnd) As SprintEnd,
                 w.System_Title As SprintName,
                 c.Conchango_VSTS_Scrum_SprintNumber As SprintNumber
FROM       [Current Work Item] AS c
                INNER JOIN [Team Project] AS t ON c.[Team Project] = t.__ID
                INNER JOIN [Work Item] AS w ON c.[Work Item] = w.__ID
WHERE    w.System_WorkItemType ='Sprint' AND '/'+t.[Team Project]+'/Portal Reports'  = @vProjectName
AND         w.System_State != 'Deleted'
),

-- ----------------------------------------------------------
-- And the Product Backlog Items
-- ----------------------------------------------------------
CTE4 AS
(
SELECT    c.Conchango_VSTS_Scrum_EstimatedEffort As Effort,
                w.System_Id AS SystemId
FROM      [Current Work Item] AS c
                INNER JOIN [Team Project] AS t ON c.[Team Project] = t.__ID
                INNER JOIN [Work Item] AS w ON c.[Work Item] = w.__ID
WHERE    w.System_WorkItemType = 'Product Backlog Item' AND '/'+t.[Team Project]+'/Portal Reports'  = @vProjectName
AND         w.System_State != 'Deleted'
),

-- ----------------------------------------------------------
-- Join the Product Backlogs to the History to find when/if each was completed
-- ----------------------------------------------------------
CTE5 AS
(
SELECT Effort,
             a.SystemId,
             b.StartDate As CompletedDate
FROM CTE4 As a
LEFT OUTER JOIN CTE2 As b ON a.SystemId = b.SystemId
),

-- ----------------------------------------------------------
-- Join again to the Sprints to get a Spring/Backlog combination for all sprints where the backlog is completed
-- ----------------------------------------------------------
CTE6 AS
(
SELECT Effort,
             SprintNumber,
             SprintName,
             SprintStart,
             SprintEnd
FROM CTE5 AS a
INNER JOIN CTE3 AS b ON a.CompletedDate &lt;= b.SprintEnd
),

-- ----------------------------------------------------------
-- Get all Product Backlog history where the Estimated Effort is positive
-- ----------------------------------------------------------
CTE7 AS
(
SELECT     YEAR(h.Date)*10000+MONTH(h.Date)*100+DAY(h.Date) AS StartDate,
                 w.System_Id AS SystemId,
                 h.Conchango_VSTS_Scrum_EstimatedEffort AS Effort
FROM         [Work Item History] AS h
INNER JOIN [Work Item] AS w ON h.[Work Item] = w.__ID
INNER JOIN [Team Project] AS t ON h.[Team Project] = t.__ID
WHERE    w.System_WorkItemType = 'Product Backlog Item' AND '/'+t.[Team Project]+'/Portal Reports'  = @vProjectName
AND         w.System_State != 'Deleted'
AND         h.Conchango_VSTS_Scrum_EstimatedEffort &gt; 0
),

-- ----------------------------------------------------------
-- Group by SystemId/Effort to retrieve the earliest from each set of duplicates
-- ----------------------------------------------------------
CTE8 AS
(
SELECT SystemId, Effort, MIN(StartDate) As StartDate FROM CTE7 GROUP BY SystemId, Effort
),

-- ----------------------------------------------------------
-- Join to the Sprints to get all records for each
-- Sprint where the date is earlier than the start
-- of the Sprint
-- ----------------------------------------------------------
CTE10 AS
(
SELECT SystemId,
             Effort,
             SprintName,
             SprintEnd,
             StartDate
FROM CTE8 AS a
INNER JOIN CTE3 AS b ON a.StartDate &lt;= b.SprintStart
),

CTE10a AS
(
SELECT SystemId,
             SprintEnd,
             MAX(StartDate) AS StartDate
FROM CTE10 GROUP BY SystemId,SprintEnd
),

-- ----------------------------------------------------------
-- Group in order to retrieve the latest from each
-- set per Backlog/Sprint combination
-- ----------------------------------------------------------
CTE11 AS
(
SELECT a.SystemId,
             Effort,
             SprintName,
             a.SprintEnd
FROM CTE10 AS a
INNER JOIN CTE10a AS b ON a.SystemId=b.SystemId AND a.SprintEnd=b.SprintEnd AND a.StartDate=b.StartDate
),

-- ----------------------------------------------------------
-- Sum the effort remaining at the start of each sprint
-- ----------------------------------------------------------
CTE12 AS
(
SELECT SUM(Effort) AS EffortAtStart,
             SprintName,
             SprintEnd
FROM CTE11 GROUP BY SprintName, SprintEnd
),

-- ----------------------------------------------------------
-- Sum all the effort completed for each Sprint
-- ----------------------------------------------------------
CTE13 AS
(
SELECT SUM(Effort) AS EffortCompleted,
             SprintName,
             SprintStart,
             SprintEnd
FROM CTE6 GROUP BY SprintName, SprintStart, SprintEnd
),

-- ----------------------------------------------------------
--Join the two together
-- ----------------------------------------------------------
CTE14 AS
(
SELECT EffortAtStart,
             EffortCompleted,
             EffortAtStart-EffortCompleted AS EffortAtEnd,
             a.SprintName,
             a.SprintStart,
             a.SprintEnd
FROM CTE13 AS a
INNER JOIN CTE12 AS b ON a.SprintEnd=b.SprintEnd
),

-- ----------------------------------------------------------
-- Null out data for any Sprints that haven't finished yet
-- ----------------------------------------------------------
CTE15 AS
(
SELECT EffortAtStart,
             CASE WHEN SprintEnd &lt;= YEAR(GETDATE())*10000+MONTH(GETDATE())*100+DAY(GETDATE())
                       THEN EffortCompleted
                       ELSE NULL
             END AS EffortCompleted,
            CASE WHEN SprintEnd &lt;= YEAR(GETDATE())*10000+MONTH(GETDATE())*100+DAY(GETDATE())
                       THEN EffortAtEnd
                       ELSE NULL
             END AS EffortAtEnd,
             SprintName,
             SprintStart,
             SprintEnd
FROM CTE14
),

-- ----------------------------------------------------------
-- Find the earliest sprint end
-- ----------------------------------------------------------
CTE16 AS
(
SELECT MIN(SprintEnd) AS SprintEnd FROM CTE15
),

-- ----------------------------------------------------------
-- Add an artificial sprint record at the front to
-- hold the initial position
-- ----------------------------------------------------------
CTE17 AS
(
SELECT EffortAtStart,
             0 AS EffortCompleted,
             EffortAtStart As EffortAtEnd,
             '' AS SprintName,
             0 AS SprintStart,
             a.SprintEnd-1 AS SprintEnd
FROM CTE14 AS a
INNER JOIN CTE16 AS b ON a.SprintEnd=b.SprintEnd
UNION
SELECT EffortAtStart,
             EffortCompleted,
             EffortAtEnd,
             SprintName,
             SprintStart,
             SprintEnd
FROM CTE15
),

-- ----------------------------------------------------------
-- Concatenate all the effort values into a string to pass to the C# code
-- ----------------------------------------------------------
CTE18 AS
(
SELECT (SELECT CAST(EffortAtEnd AS varchar) + ',' AS [text()]
              FROM CTE17
              ORDER BY SprintEnd
              FOR XML PATH('')
              ) AS ConcatenatedBurndown
),

-- ----------------------------------------------------------
-- Add the concatenated string to each record
-- ----------------------------------------------------------
CTE19 AS
(
SELECT EffortAtStart,
             EffortCompleted,
             EffortAtEnd,
             SprintName,
             SprintStart,
             SprintEnd,
             ConcatenatedBurndown,
             ROW_NUMBER() OVER(ORDER BY SprintEnd) AS Row
FROM CTE17
INNER JOIN CTE18 ON 1=1
),

-- ----------------------------------------------------------
-- Join table to itself in
-- order to calculate the difference between work 
-- completed in adjoining Sprints, i.e. the work done
-- by each Sprint
-- ----------------------------------------------------------

CTE20 AS
(
SELECT a.EffortAtStart,
             a.EffortCompleted,
             a.EffortAtEnd,
             a.SprintName,
             a.SprintStart,
             a.SprintEnd,
             a.ConcatenatedBurndown,
             CASE WHEN a.EffortAtEnd IS NULL
                       THEN NULL
                       ELSE  a.EffortCompleted - b.EffortCompleted
             END AS WorkDone
FROM CTE19 AS a
INNER JOIN CTE19 AS b ON b.Row=a.Row-1
)

SELECT * FROM CTE20 ORDER BY SprintEnd</CommandText>
